generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        Int    @id @default(autoincrement())
  name      String
  direction String
  gender    String
  phone     String @unique
}

model Admin {
  id       Int    @id @default(autoincrement())
  password String
  type     String
}
