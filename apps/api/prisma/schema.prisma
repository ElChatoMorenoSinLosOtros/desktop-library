generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        Int    @id @default(autoincrement())
  name      String
  direction String
  phone     String @unique
}

model Admins {
  id        Int     @id @default(autoincrement())
  email     String  @unique
  password  String
  name      String
  role      String
}
